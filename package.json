{
  "private": true,
  "scripts": {
    "start": "set PORT=8003&umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@types/simplemde": "^1.11.7",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.3.2",
    "antd": "^3.23.0",
    "braft-editor": "^2.3.9",
    "dingtalk-jsapi": "^2.13.9",
    "easymde": "^2.13.0",
    "highlight.js": "^10.4.1",
    "marked": "^1.2.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-simplemde-editor": "^4.1.3",
    "simplemde": "^1.11.2",
    "umi": "^3.3.2",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1"
  }
}
